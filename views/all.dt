extends ./layout

block content
  - import f3lcites.citedata;
  p#desc Alle #{llen} bekannten Zitate.
  div#citebody
    - foreach(FullCiteData cite; cites)
      div.citecontainer
        div.citeid #{cite.id}
        div.cite #{cite.cite}
        div.clear
        div.added #{cite.added}
        div.addedby #{cite.addedby}
        div.changed #{cite.changed}
        div.changedby #{cite.changedby}
